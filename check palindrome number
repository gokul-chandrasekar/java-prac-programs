public class Main{
    public static void main(String args[]){
        int r,sum=0;
        int n=643;
        int temp=n;
        while(n>0){
            r=n%10;
            sum=(sum*10)+r;
            n=n/10;
        }
        if(sum==temp){
            System.out.print("Palindrome number.");
        }
        else{
            System.out.print("not a palindrome");
        }
    }
}
