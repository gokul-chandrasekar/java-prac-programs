public class Main{
    public static void main(String args[]){
        String n="man",revstr=" ";
        for(int i=n.length()-1;i>=0;i--){
            revstr=revstr+n.charAt(i);
        }
        if(n.toLowerCase().equals(revstr.toLowerCase())){
            System.out.println("its a palindrome");
        }
        else{
            System.out.print("not a palindrome");
        }
    }
}
